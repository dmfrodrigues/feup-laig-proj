<lsf>
    <initials>
        <reference length="1.0"/>
        <root id="root"/>
    </initials>
    <views default="closer_camera">
        <!-- at least one of the following type of cameras must be declared -->
        <perspective id="perspective_camera" near="0.1" far="500" angle="45">
            <from x="10.0" y="3.0" z="10.0"/>
            <to   x= "0.0" y="1.0" z= "0.0"/>
        </perspective>
        <perspective id="closer_camera" near="0.1" far="500" angle="45">
            <from x="1.6" y="1.4" z="1.6"/>
            <to   x="0.0" y="1.0" z="0.0"/>
        </perspective>
        <ortho id="ortho_camera" near="0.2" far="100" left="-0.2" right="0.2" top="0.2" bottom="-0.2">
            <from x="5" y="0" z="10"/>
            <to x="5" y="0" z="0"/>
            <up x="0" y="1" z="0"/><!-- optional, default 0,1,0 -->
        </ortho>
    </views>
    <illumination>
        <ambient    r="0.2" g="0.2" b="0.2" a="1.0"/>
        <background r="0.1" g="0.7" b="1.0" a="1.0"/>
    </illumination>
    <lights>
        <!-- NOTE: at least one light should be present, and a maximum of eight lights is accepted. -->
        <light id="ceiling-light">
            <enable value="1"/>
            <position x="0.0" y="2.0" z="0.0" w="1.0"/>
            <ambient  r="0.0" g="0.0" b="0.0" a="1.0"/>
            <diffuse  r="1.00" g="0.96" b="0.90" a="1.0"/>
            <specular r="1.00" g="0.96" b="0.90" a="1.0"/>
        </light>
    </lights>
    <textures>
        <!-- NOTE: this block "TEXTURE" must be repeated as necessary with different "id" -->
        <texture id="black_painted_metal" path="./scenes/images/black-painted-metal.jpg"/>
        <texture id="plastic-rugged"      path="./scenes/images/plastic-edited.jpg"     />
        <texture id="tabletop"            path="./scenes/images/white-tabletop.jpg"     />
        <texture id="world_map"           path="./scenes/images/world-map.jpg"          />

        <texture id="book-pages"          path="./scenes/images/book-pages.jpg"         />

        <texture id="intro-cg-front"      path="./scenes/images/intro-cg-front.jpg"     />
        <texture id="intro-cg-back"       path="./scenes/images/intro-cg-back.jpg"      />
        <texture id="intro-cg-spine"      path="./scenes/images/intro-cg-spine.jpg"     />
        <texture id="intro-cg-stripe"     path="./scenes/images/intro-cg-stripe.jpg"    />

        <texture id="floor"               path="./scenes/images/floor.jpg"              />
        <texture id="wall"                path="./scenes/images/wall.jpg"               />
        <texture id="ceil"                path="./scenes/images/ceil.jpg"               />
    </textures>
    <materials>
        <!-- At least one material should be present. -->
        <material id="plastic-white">
            <shininess value="5.0"/>
            <ambient  r="1.0" g="1.0" b="1.0" a="1.0"/>
            <diffuse  r="1.0" g="1.0" b="1.0" a="1.0"/>
            <specular r="0.9" g="0.9" b="0.9" a="1.0"/>
            <emissive r="0.0" g="0.0" b="0.0" a="1.0"/>
        </material>
        <material id="plastic-pink">
            <shininess value="1.0"/>
            <ambient  r="0.15" g="0.11" b="0.23" a="1.0"/>
            <diffuse  r="0.15" g="0.11" b="0.23" a="1.0"/>
            <specular r="0.05" g="0.04" b="0.08" a="1.0"/>
            <emissive r="0.0" g="0.0" b="0.0" a="1.0"/>
        </material>
        <material id="plastic-yellow">
            <shininess value="1.0"/>
